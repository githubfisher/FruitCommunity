/* pages/my/my.wxss */
.u-bg{
  width:100%;
}
.u-info{
  height: 100px;
  margin:10px;
  background-color:#e22319;
  border-radius:5px;
  font-size:14px;
  color:#fff;
}
.u-avatar{
  float: left;
  padding: 10px;
}
.u-avatar image{
  width:128rpx;
  height:128rpx;
  margin:20rpx;
  border-radius:50%;
}
.u-level {
  font-size: 11px;
  color: #fff;
  margin-top:-34px;
  margin-left:-10px;
}
.u-level image {
  width: 20px;
  height: 20px;
  float: left;
  z-index: 100;
  position:relative;
}
.u-level text{
  line-height: 40px;
  background-color:#888;
  padding:1px 12px;
  border-radius:10px;
  margin-left:-16px;
}
.u-login{
  line-height:100px;
}
.u-detail{
  padding-top: 32px;
}
.my-list{
  width: 100%;
}
.list-li{
  padding:10px 10px 10px 0;
  font-size:14px;
  border-top: 1px #e5e9ec solid;
  margin: 0 0 0 10px;
}
.list-li text{
  padding-left:5px;
}